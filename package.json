{
  "name": "@anth0nycodes/fabric-history",
  "version": "0.1.4",
  "description": "A library built on top of fabric.js that allows for easy access to canvas history.",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "start": "node dist/index.js",
    "check": "tsc --noEmit",
    "publish:npm": "pnpm publish --access public"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "types": "dist/index.d.ts",
  "keywords": [
    "fabric",
    "fabric.js",
    "canvas history",
    "canvas undo",
    "canvas redo",
    "canvas"
  ],
  "author": "Anthony Hoang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anth0nycodes/fabric-history.git"
  },
  "bugs": {
    "url": "https://github.com/anth0nycodes/fabric-history/issues"
  },
  "homepage": "https://github.com/anth0nycodes/fabric-history#readme",
  "peerDependencies": {
    "fabric": ">=6.0.0 <8.0.0"
  },
  "pnpm": {
    "overrides": {
      "rollup": ">=4.59.0"
    }
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@vitest/coverage-v8": "^4.0.18",
    "canvas": "^3.2.1",
    "jsdom": "^28.0.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
